<!DOCTYPE html>
<html lang="en">
<%- include ('globals/head') %>
<%- include ('globals/navbar') %>
<body>
    <div class="container">
        <%for(let post of posts){ %>
            <article>
                <img src="<%= post.img %> " alt="">
                <h2><%= post.title %></h2>
                <p><%= post.intro %></p>
                <h2><%=post.head1 %></h2>
                <p><%= post.content %></p>
                <h2><%= post.head2 %></h2>
                <p><%= post.conclusion %></p>
            </article>
        <%}%>
    </div>
    
</body>
</html>